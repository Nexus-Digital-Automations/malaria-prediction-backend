            "gridPos": {"h": 8, "w": 8, "x": 8, "y": 8}
          },
          {
            "id": 5,
            "title": "üîó Load Average",
            "type": "timeseries",
            "targets": [
              {
                "expr": "malaria_system_cpu_load_average{period=\"1min\"}",
