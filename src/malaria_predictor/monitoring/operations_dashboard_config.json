              },
              {
                "expr": "malaria_system_cpu_load_average{period=\"5min\"}",
                "legendFormat": "5min Load"
              },
              {
                "expr": "malaria_system_cpu_load_average{period=\"15min\"}",
                "legendFormat": "15min Load"
              }
            ],
            "fieldConfig": {
              "defaults": {
                "unit": "short",
                "custom": {
                  "drawStyle": "line",
                  "lineInterpolation": "smooth",
