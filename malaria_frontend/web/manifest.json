    "related_applications": [
        {
            "platform": "play",
            "url": "https://play.google.com/store/apps/details?id=com.malariaprediction.app",
            "id": "com.malariaprediction.app"
        },
        {
            "platform": "itunes",
            "url": "https://apps.apple.com/app/malaria-prediction/id123456789",
            "id": "123456789"
        }
    ],
    "protocol_handlers": [
        {
            "protocol": "web+malaria",
            "url": "/handle-protocol?type=%s"
        }
    ],
    "file_handlers": [
        {
            "action": "/handle-file",
            "accept": {
                "application/x-malaria-prediction-report": [".mpr"],
                "text/csv": [".csv"],
                "application/json": [".json"]
            }
        }
    ],
    "share_target": {
        "action": "/share",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url",
            "files": [
                {
                    "name": "data",
                    "accept": ["text/csv", "application/json", ".mpr"]
                }
            ]
        }
    },
    "launch_handler": {
        "client_mode": "navigate-existing"
    },
    "edge_side_panel": {
        "preferred_width": 400
    },
    "handle_links": "preferred",
    "capture_links": "new-client",
    "tab_strip": {
        "home_tab": {
            "icons": [
                {
                    "src": "icons/tab-home.png",
                    "sizes": "16x16",
                    "type": "image/png"
                }
            ]
        }
    },
    "permissions": [
        "geolocation",
        "notifications",
        "persistent-storage",
        "background-sync"
    ]
}
